{
   "extends":"airbnb",
   "rules":{
      "react/jsx-filename-extension":[
         1,
         {
            "extensions":[
               ".js",
               ".jsx"
            ]
         }
      ],
      "jsx-a11y/href-no-hash":"off",
      "jsx-a11y/anchor-is-valid":[
         "warn",
         {
            "aspects":[
               "invalidHref"
            ]
         }
      ],
      "prefer-destructuring":[
         "error",
         {
            "array":false,
            "object":true
         },
         {
            "enforceForRenamedProperties":false
         }
      ]
   },
   "globals":{
      "document":true,
      "window":true,
      "navigator":true,
      "fetch":true,
      "Request":true,
      "Headers":true
   }
}